<script language="JavaScript">

    "use strict";

    // Global vars
    // Should minimize them

    //        var solrPopbioUrl = 'http://vb-dev.bio.ic.ac.uk:9779/asolr/solr/vb_popbio/';
    //        var solrTaUrl = 'http://vb-dev.bio.ic.ac.uk:9779/asolr/solr/vb_ta/';
    //    var solrPopbioUrl = 'https://pre.vectorbase.org/popbio/map/asolr/solr/vb_popbio/';
    var solrPopbioUrl = 'asolr/solr/vb_popbio/';
    //    var solrPopbioUrl = 'http://funcgen.vectorbase.org/popbio-map-preview/asolr/solr/vb_popbio/';
    //    var solrTaUrl = 'https://pre.vectorbase.org/popbio/map/asolr/solr/vb_ta/';
    var solrTaUrl = 'asolr/solr/vb_ta/';
    //    var solrTaUrl = 'http://funcgen.vectorbase.org/popbio-map-preview/asolr/solr/vb_ta/';
    var map, sidebar;
    var assetLayerGroup;
    var smallClusters = [], months = [];
    var palette;
    var legendSpecies = 20;
    var qryUrl = 'q=*';
    var partSearch = false;
    var legend;
    var PaneSpinner = null;
    var highlight = null, rectHighlight = null, firstClick = true;
    var highlightedId = false, sidebarClick = false, selectedPlotType = 'none'; // these are to help refresh the panel properly after clicking active terms
    var geohashesGrid = null;
    // map scaled values from 0-1 to colors green to red
    //    var trafficLight = new L.HSLHueFunction(new L.Point(0,120), new L.Point(1,0));
    var colorBrewer = L.ColorBrewer.Diverging.RdYlBu[10].slice(); // using slice() to copy array by value
    var trafficLight = new L.CustomColorFunction(0, 1, colorBrewer, {interpolate: true});

    // Hack to prevent single click events firing when double clicking
    var timer = 0;
    var delay = 400;

    var prevent = false, stickyHover = false;
    // this is to remember background type selection between markers
    var bgPlotType = 1;

    // Norm IR slider initiation
    var normIrSlider;
    var resizeId, previousWindowHeight;


    initializeSearch();

    // set view according to the value of the view url parameter. Otherwise default to smpl
    var urlParams = $.parseParams();

    //      FixMe: kind of an ugly hack to set the dropdown text.

    $(window).load(function () {

        var windowHeight = $(window).height();
        $('#map_container').height(windowHeight - 166 - 55);
        previousWindowHeight = windowHeight;

        $('.sidebar-pane').data('has-graph', false);


        $(document).on("click", '.active-term', function () {
                    highlightedId = $('.highlight-marker').attr('id');

                    if ($('.sidebar-pane.active').attr('id') === 'swarm-plots') {
                        selectedPlotType = $('#plotType').val();

                    } else {
                        $('#plotType').val('none');
                    }

                    $('#search_ac').tagsinput('add', {
                        value: $(this).attr('value'),
                        activeTerm: true,
                        type: $(this).attr('type'),
                        field: mapTypeToField($(this).attr('type')),
                        qtype: 'exact'

                    });

                    var tooltip = d3.select('#beeswarmPointTooltip');
                    if ($('#no-interactions').hasClass("foreground")) {

                        tooltip.transition()
                                .duration(500)
                                .style("opacity", 0)
                                .style("z-index", -1000000);
                        $('#no-interactions').removeClass("foreground");
                        stickyHover = false;

                    }
                })

                .on("click", '.active-legend', function () {

                    $('#search_ac').tagsinput('add', {
                        value: $(this).attr('value'),
                        activeTerm: true,
                        type: $(this).attr('type'),
                        field: mapTypeToField($(this).attr('type')),
                        qtype: 'exact'

                    });

                    var tooltip = d3.select('#beeswarmPointTooltip');
                    if ($('#no-interactions').hasClass("foreground")) {

                        tooltip.transition()
                                .duration(500)
                                .style("opacity", 0)
                                .style("z-index", -1000000);
                        $('#no-interactions').removeClass("foreground");
                        stickyHover = false;

                    }
                })
                // This is here to trigger an update of the graphs when an active-term is clicked
                // FixMe: Have to solve the issue with pruneclusters first
                .on("jsonLoaded", function () {
                    if (highlightedId) {
                        var marker = $('#' + highlightedId);
                        if (marker.length > 0) {
                            $(marker).trigger("click");
                            highlightMarker(marker);
                        } else {
                            sidebar.close();
                            setTimeout(function () {
                                resetPlots()
                            }, delay);
                        }
                        highlightedId = false;
                    }

                });


        // trigger click event on highlighted marker when switching panels
        $('.sidebar-icon').on("click", function () {
            var highlightedMarker = $('.highlight-marker');
            if (highlightedMarker) {
                sidebarClick = true;
                $(highlightedMarker).trigger("click");
                sidebarClick = false;
            }
        });

        // add the IR scale bars in the advanced options pane
        var inHtml = '';
        $.each(colorBrewer.slice().reverse(), function (index, value) {
            inHtml += '<i style="margin: 0; border-radius: 0; border: 0; color: ' + value + '; width: 10%; background-color: ' + value + ' ;"></i>';
        });
        $('#menu-scale-bars').html(inHtml);

        normIrSlider = $('#pre-norm-ir-slider').bslider({
            value: [0, 9],
            id: "norm-ir-slider",
            handle: "triangle",
            min: 0,
            max: 9,
            step: 1,
            tooltip: 'hide'
        });

        bindEvents();
        initializeMap();

        // detect when user changes zoom or pans around the map
        map.on("movestart", function () {
                    removeHighlight();
                    // close open panels
                    $('.collapse').collapse('hide');
                    sidebar.close();
                    setTimeout(function () {
                        resetPlots()
                    }, delay);
                })
                .on("moveend", function () {

                    loadSolr({clear: 1, zoomLevel: map.getZoom()});
                })
                .on("click", function () {
                    removeHighlight();
                    sidebar.close();
                    // close open panels
                    $('.collapse').collapse('hide');
                    setTimeout(function () {
                        resetPlots()
                    }, delay);
                });

    });


    $(window).resize(function () {
        clearTimeout(resizeId);
        resizeId = setTimeout(doneResizing, 500);
    });


    function doneResizing() {
        //whatever we want to do
        var windowHeight = $(window).height();
        if (windowHeight != previousWindowHeight) {
            $('#map_container').height(windowHeight - 166 - 55);
            previousWindowHeight = windowHeight;
            map.invalidateSize();


        }
    }

</script>







