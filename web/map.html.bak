<script type="text/x-jsrender" id="smplRowTemplate">
    <div class="row no-pad marker-row">


        <div class="col-md-12" >
            <h3 style="background-color: {{attr:bgColor}}; color: {{attr:textColor}};">
            <span class="active-term" type="{{attr:speciesType}}" value="{{attr:species}}">
                <i>{{html:species}}</i></h3>
            </span>
            <p>{{html:bundleName}}
                <span class="active-term" type="{{attr:accessionType}}" value="{{attr:accession}}">
                    {{html:accession}}
                </span>
                <a href="http://www.vectorbase.org{{url:url}}" target="_blank">
                    <i class="fa fa-external-link fa-fw" aria-hidden="true"></i>
                </a>
                <span class="active-term" type="{{attr:sampleTypeType}}" value="{{attr:sampleType}}">
                    ({{html:sampleType}})
                </span>
            </p>
        </div>
        <div class="row">
            <div class="col-md-6">
                <p>
                    <i class="fa fa-calendar fa-fw" title="Collection date" style="color: {{attr:bgColor}};"></i>
                    {{for collectionDate}}
                        {{>#data}}
                    {{else}}
                        No date
                    {{/for}}
                </p>
            </div>
            <div class="col-md-6">
                <p>
                    <i class="fa fa-map-marker fa-fw" title="Coordinates: {{html:geoCoords}}" style="color: {{attr:bgColor}};"></i>
                    <span class="active-term" type="{{attr:geolocationType}}" value="{{attr:geolocation}}">
                        {{html:geolocation}}
                    </span>
                 </p>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <p style="color: {{attr:bgColor}};">
                    <i class="fa fa-database fa-fw" title="Projects"></i><b> Project(s)</b>
                </p>
                <ul>
                {{for projects}}
                    <li>
                        <span class="active-term" type="Projects" value="{{attr:#data}}">
                            {{>#data}}
                        </span>
                        <a href="http://www.vectorbase.org/popbio/project/?id={{>#data}}" target="_blank">
                            <i class="fa fa-external-link fa-fw" aria-hidden="true"></i>
                        </a>
                    </li>
                {{/for}}
                </ul>
            </div>
            <div class="col-md-6">
                <p style="color: {{attr:bgColor}};">
                    <i class="fa fa-shopping-cart fa-fw" title="Collection Protocols"></i><b> Col. Protocol(s)</b>
                </p>
                <ul>
                {{for collectionProtocols}}
                    <li>
                        <span class="active-term" type="Collection protocols" value="{{attr:#data}}">
                            {{>#data}}
                        </span>
                    </li>
                {{/for}}
                </ul>
            </div>
        </div>
    </div>







</script>

<script type="text/x-jsrender" id="irRowTemplate">
    <div class="row no-pad marker-row">


        <div class="col-md-12" >
            <h3 style="background-color: {{attr:bgColor}}; color: {{attr:textColor}};">
                <span class="active-term" type="{{attr:speciesType}}" value="{{attr:species}}">
                    <i> {{html:species}}</i>
                </span>
            </h3>
            <p>{{html:bundleName}}
            <a href="http://www.vectorbase.org{{url:url}}" target="_blank">{{html:accession}}</a>
            {{if sampleSize}}
                <span class="active-term" type="{{attr:sampleTypeType}}" value="{{attr:sampleType}}">
                    ({{html:sampleType}}
                </span>
                 of {{>sampleSize}} individuals)</p>
            {{else}}
                <span class="active-term" type="{{attr:sampleTypeType}}" value="{{attr:sampleType}}">
                    ({{html:sampleType}})
                </span></p>
            {{/if}}
        </div>
        <div class="row">
            <div class="col-md-6">
                <p><i class="fa fa-calendar" title="Collection date" style="color: {{attr:bgColor}};"></i>
                {{for collectionDate}}
                    {{>#data}}
                {{else}}
                    No date
                {{/for}}
                </p>
            </div>
